<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="wxNano Library" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="shared">
				<Option output="shared/wxNano" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/shared/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#WX31_64.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="$(#WX31_64.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
			</Target>
			<Target title="static">
				<Option output="static/wxNano" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/static/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="$(#WX31_64.lib)/gcc_lib$(WX_CFG)/msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add library="wxbase$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="$(#WX31_64.lib)/gcc_lib$(WX_CFG)" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="31" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="shared;static;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fpermissive" />
			<Add directory="../include" />
			<Add directory="$(#WX31_64.include)" />
		</Compiler>
		<Linker>
			<Add library="opencv_core" />
		</Linker>
		<Unit filename="../include/wxNano.h" />
		<Unit filename="../src/number.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
